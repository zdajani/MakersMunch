  <body class= "body">
    <h1 id="title"> Munch </h1>
    <br>
    <%if current_user %>
      <form method='post' action='/log_in'>
        <input type='hidden' name='_method' value='delete'>
        <input type='submit' value='Log Out' class="Log_out">
      </form>
    <%else%>
      <a href="/user/new" class="button">Sign Up</a>
      <a href="/log_in" class="log_button">Log In</a>
    <%end%>

    <% if flash[:notice] %>
      <div id='notice'>
        <%= flash[:notice] %>
      </div>
    <% end %>

    <form action="/dropdown" method="post">
      <select name="food_tags">
        <option>Pizza</option>
        <option>Pasta</option>
        <option>Burger</option>
        <option>Street Food</option>
        <option>Chicken</option>
        <option>Chinese</option>
        <option>Sushi</option>
        <option>Mexican</option>
        <option>Japanese</option>
        <option>Sandwich</option>
        <option>Fast Food</option>
        <option>Indian</option>
        <option>Thai</option>
        <option>Cafe</option>
        <option>Pub</option>
      </select>
      <input type="submit" value="Find!">
    </form>

    <div id='map'> </div>
    <div id="boxed" style="overflow-y:scroll">
      <% @restaurants.each do |restaurant| %>
        <script>
          var place_id = '<%=restaurant.place_id%>';
          getApiDetails(place_id);
        </script>
      <% end %>
      </div>
    </div>
  </body>
</html>
